apiVersion: v1
kind: Service
metadata:
  labels:
    run: auditsafe-ds
  name: auditsafe-ds
  namespace: _NAMESPACE_
spec:
  externalTrafficPolicy: Cluster
  ports:
  - port: _DS_PORT_
    protocol: TCP
    targetPort: 6060
  selector:
    run: auditsafe-ds
  sessionAffinity: None
  type: LoadBalancer
  loadBalancerIP: _PUBLIC_DS_IP_
status:
  loadBalancer: {}
